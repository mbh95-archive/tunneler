{
  "name": "tunneler",
  "version": "1.0.0",
  "description": "Tunneler",
  "scripts": {
    "client:build": "webpack --config webpack-client.prod.js",
    "client:watch": "webpack-dev-server --open --config webpack-client.dev.js",
    "server:build": "tsc --project src/server",
    "server:watch": "tsc-watch --project src/server --onSuccess \"node dist/server/main.js\"",
    "server:start": "node dist/server/main.js"
  },
  "author": "Ashok Sankaran and Matt Hammond",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/express-ws": "^3.0.0",
    "@types/flatbuffers": "^1.10.0",
    "@types/ws": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.4.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon-webpack-plugin": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.0.4",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-ws": "^4.0.0",
    "flatbuffers": "^1.11.0",
    "ws": "^7.2.1"
  }
}
